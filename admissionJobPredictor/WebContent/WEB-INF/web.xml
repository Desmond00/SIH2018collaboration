  </servlet>
  <servlet-mapping>
  	<servlet-name>admissionHome</servlet-name>
  	<url-pattern>/admissionHome</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>instituteList</servlet-name>
